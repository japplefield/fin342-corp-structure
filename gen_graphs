./file_import.py

mkdir 'Debt Bar Charts'
./debt_bar_charts.py

mkdir 'Debt Cumulative Line Charts'
./debt_cumulative_line_graphs.py

mkdir 'Equity Bar Charts'
for FILE in $(ls | egrep '^equity(.+)bar')
do
    ./$FILE
done

mkdir 'Equity Cumulative Line Charts'
for FILE in $(ls | egrep '^equity(.+)cumulative')
do
    ./$FILE
done
